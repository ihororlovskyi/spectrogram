{
  "name": "vite-typescript-starter",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "dev:all": "concurrently -k -n VITE,API -c green,blue \"npm run dev\" \"npm run dev:api\"",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "concurrently": "^9.2.1",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  },
  "engines": {
    "node": "24.11.1",
    "npm": "11.6.2"
  }
}
